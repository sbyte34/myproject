<!doctype html>
<html lang="en">
  <head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    {{!-- bootstrap css cdn link --}}
        <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/css/bootstrap.min.css" rel="stylesheet">
        <link href='https://unpkg.com/boxicons@2.1.1/css/boxicons.min.css' rel='stylesheet'>
        <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.5.0/css/font-awesome.min.css">
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/OwlCarousel2/2.3.4/assets/owl.carousel.min.css">
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/OwlCarousel2/2.3.4/assets/owl.theme.default.min.css">
        <link rel="stylesheet" href="/static/css/style.css">
        <link rel="icon" type="image/x-icon" href="/static/image/smartbyte_circleimg.png">
        
    <title>SmartByte Computer Education</title>
  </head>
  <body>
    {{>navbar}}
     {{!-- section one image and registration page --}}
    <div class="container-fluid">
        <div class="row no-gutters shadow-lg">
            <div class="col-md-6 d-md-block">
                <img src="/static/image/image1.jpg" class="img-fluid"
                    style="min-height:70%; border-radius: 15px; margin-top:60px;" />
            </div>
            <div class="col-md-6 bg-white p-5">
            <div class="form-style">
                    <form class="p-lg-2 col-12 row g-3" action="https://formspree.io/f/xbjbwnza" method="post">
                        <div class="col-lg-6">
                            <label for="userName" class="form-label">First name</label>
                            <input type="text" class="form-control" placeholder="Ex.Divya" name="firstname" id="fName"
                                aria-describedby="emailHelp">
                        </div>
                        <div class="col-lg-6">
                            <label for="userName" class="form-label">Last name</label>
                            <input type="text" class="form-control" placeholder="Ex.Banode" name="lastname" id="lName"
                                aria-describedby="emailHelp">
                        </div>
                        <div class="col-12">
                            <label for="userName" class="form-label">Email address</label>
                            <input type="email" class="form-control" placeholder="divyabanode@example.com" name="email"
                                id="email" aria-describedby="emailHelp">
                        </div>
                        <div class="col-12">
                            <label for="userName" class="form-label">Contact Number</label>
                            <input type="number" class="form-control" placeholder="+91 12345 67890" name="phno" id="cno"
                                aria-describedby="emailHelp">
                        </div>
                        <div class="col-12">
                            <label for="exampleInputEmail1" class="form-label">Write Enquiry</label>
                            <textarea name="textarea" placeholder="Enquiry" class="form-control" name="massage" id="msg"
                                rows="4"></textarea>
                        </div>
                        <div class="col-12">
                            <button type="submit"  class="btn btn-brand">Send Enquiry</button>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
     <a href="https://web.whatsapp.com/" class="float" target="_blank"><i class="fa fa-whatsapp my-float"></i></a>
    {{>about}}
    {{>slider slides=slides}}
    {{>branch br_name=br_name}}
     {{!-- {{>features}} --}}
    {{>footer}}
    
  


    
    
    
    
    
    
